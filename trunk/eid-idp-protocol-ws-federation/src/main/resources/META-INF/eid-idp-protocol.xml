<?xml version="1.0" encoding="UTF-8"?>
<IdentityProviderProtocols xmlns="urn:be:fedict:eid:idp:spi:protocol">
    <IdentityProviderProtocol>
        <Name>WS-Federation Protocol Service</Name>
        <Description>This eID IdP Protocol Service provides an WS-Federation
            protocol for eID authentication and identification purposes.
        </Description>
        <ProtocolService>be.fedict.eid.idp.protocol.ws_federation.WSFederationProtocolService</ProtocolService>
        <ContextPath>/ws-federation</ContextPath>
        <Endpoints>
            <Endpoint>
                <ServletClass>be.fedict.eid.idp.protocol.ws_federation.WSFederationMetadataHttpServlet</ServletClass>
                <ContextPath>/ws-federation-metadata.xml</ContextPath>
            </Endpoint>
        </Endpoints>
    </IdentityProviderProtocol>
</IdentityProviderProtocols>